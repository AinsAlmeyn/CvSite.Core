@model Home
@{
    ViewData["Title"] = "NewHomeInfo";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="table-responsive pt-3">
                    <table class="table table-bordered">
                      <thead>
                        <tr>
                          <th>
                            #
                          </th>
                          <th>
                            Ana Resim
                          </th>
                          <th>
                            Isler / Meraklar
                          </th>
                          <th>
                            Instagram Linki
                          </th>
                          <th>
                            Linkedin Linki
                          </th>
                          <th>
                            Guncelle
                          </th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr class="table-success">
                          <td>
                    @Model.Id
                          </td>
                          <td>
                    @Model.Photo
                          </td>
                          <td>
                    @Model.Jobs
                          </td>
                          <td>
                    @Model.LinkInstagram
                          </td>
                          <td>
                    @Model.LinkLinkedin
                          <td>
                    <td><a href="/NewHome/NewHomeEdit/@Model.Id" class="btn btn-outline-success">Güncelle</a></td>
                        </tr>
                      </tbody>
                    </table>
                    <br />
                  </div>